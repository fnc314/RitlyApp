<table>

	<tr><th>The List of generated Links!</th></tr>

	<tr>
		<th>No.</th>
		<th>Website URL</th>
		<th>Random String</th>
		<th>DELETE</th>
	</tr>

	<%= @links.each do |x| %>
		<tr>
			<td><%= x[:id] %></td>
			<td><%= x[:url] %></td>
			<td><%= x[:random_string] %></td>
			<td><%= link_to "DELETE", :links, :method => :delete %></td>
		</tr>
	<% end %>

</table>